{
    "rust-analyzer.rustfmt.overrideCommand": [
        "leptosfmt",
        "--stdin",
        "--rustfmt"
    ],
    "editor.formatOnSave": true,
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer"
    },
    "files.associations": {
        "*.rs": "rust"
    },
    "css.validate": false,
    "tailwindCSS.includeLanguages": {
        "rust": "html"
    },
    "emmet.includeLanguages": {
        "rust": "html"
    }
}